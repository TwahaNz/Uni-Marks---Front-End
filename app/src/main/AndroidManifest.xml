<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.tnz.com.unimarks">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.INTERACT_ACROSS_USERS_FULL"/>

    <application
        android:name=".configurations.utils.app.App"
        android:allowBackup="true"
        android:icon="@mipmap/unimarks"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activities.fragments.MainActivity"/>
        <activity android:name=".activities.StudentLogInScreen" />
        <activity android:name=".activities.StudentRegiserterPreviewScreen" />
        <activity android:name=".activities.StudentRegisterScreen" />
        <activity android:name=".activities.LoggedInScreen" />
        <activity android:name=".activities.term.DOSActivity"/>
        <activity android:name=".activities.term.IRPActivity"/>
        <activity android:name=".activities.term.ISYAActivity"/>
        <activity android:name=".activities.term.ISYBActivity"/>
        <activity android:name=".activities.term.TPActivity"/>
        <activity android:name=".activities.term.TermResults"/>

        <service android:name=".services.student.StudentLoginServiceImpl" />

        <!--
     ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

    </application>

</manifest>